<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="https://www.sitemaps.org/schemas/sitemap/0.9">
<%- _items.each do |item| -%>
   <url>
      <loc><%= item[:loc] %></loc>
      <lastmod><%= item[:lastmod] %></lastmod>
      <changefreq><%= item[:changefreq] %></changefreq>
      <priority><%= item[:priority] %></priority>
   </url>
<%- end -%>
</urlset>
